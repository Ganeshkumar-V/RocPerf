FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setExprFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


expressions
(
    alpha.propellant
    {
        field       alpha.propellant;
        dimensions  [0 0 0 0 0 0 0];

        constants
        {
        }

        variables
        (
            "xmin = 0.01"
            "xmax = 0.013"
            "zmin = 0.045"
            "zmax = 0.05"
        );

        condition
        #{
            (pos().x() >= xmin)
            && (pos().x() <= xmax)
            && (pos().z() >= zmin)
            && (pos().z() <= zmax)
            && ((pos().x() - xmin)/(pos().z() - zmin) < (xmax - xmin)/(zmax - zmin))
        #};

        expression
        #{
          0
        #};
    }

    alpha.gas
    {
        field       alpha.gas;
        dimensions  [0 0 0 0 0 0 0];

        constants
        {
        }

        variables
        (
            "xmin = 0.01"
            "xmax = 0.013"
            "zmin = 0.045"
            "zmax = 0.05"
        );

        condition
        #{
            (pos().x() >= xmin)
            && (pos().x() <= xmax)
            && (pos().z() >= zmin)
            && (pos().z() <= zmax)
            && ((pos().x() - xmin)/(pos().z() - zmin) < (xmax - xmin)/(zmax - zmin))
        #};

        expression
        #{
          0
        #};
    }
);


