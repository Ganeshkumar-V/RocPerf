FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

R_cc #calc "0.0191/2";
R_bore #calc "0.00635/2";
R_borep #calc "$R_bore + 0.0001";
R_boren #calc "$R_bore - 0.0001";
L_cc 0.0762;

T_ign 900;

defaultFieldValues
(
    volScalarFieldValue alpha.gas 1
    volScalarFieldValue alpha.particles 0
    volScalarFieldValue alpha.propellant 0
    volScalarFieldValue cellDist 0
);

regions
(
    // Propellant
    boxToCell
    {
       box ($R_bore -0.1 0) ($R_cc 0.1 $L_cc);
       fieldValues
       (
         volScalarFieldValue alpha.propellant 1.0
         volScalarFieldValue alpha.gas 0.0
       );
    }

    // Domain Decomposition - processor 0
    boxToCell
    {
	box (0 -0.1 0) ($R_cc 0.1 0.06);
	fieldValues (volScalarFieldValue cellDist 0);
    }
    
    // Domain Decomposition - processor 1
    boxToCell
    {
	box (0 -0.1 0.06) ($R_cc 0.1 0.12);
	fieldValues (volScalarFieldValue cellDist 1);
    }

    // Domain Decomposition - processor 2
    boxToCell
    {
	box (0 -0.1 0.12) ($R_cc 0.1 0.20);
	fieldValues (volScalarFieldValue cellDist 2);
    }
);

// ************************************************************************* //
