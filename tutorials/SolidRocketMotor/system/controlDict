FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rocketMotor;

startFrom       startTime;

startTime       0.001;

stopAt          endTime;

endTime         1;

deltaT          1e-8;

//writeControl    timeStep;
writeControl    adjustableRunTime;

writeInterval   1e-4;
//writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   12;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       1e-8;

DebugSwitches1
{
	SolverPerformance 	1;
}

functions
{
	fieldAverage1
	{
		type		fieldAverage;
		libs 		(fieldFunctionObjects);
	
		fields
		(
			U.gas
			{
				mean        yes;
            			prime2Mean  yes;
			        base        time;
			}
			U.particles
                        {
                                mean        yes;
                                prime2Mean  yes;
                                base        time;
                        }
			alpha.particles
                        {
                                mean        yes;
                                prime2Mean  yes;
                                base        time;
                        }
			p_rgh
                        {
                                mean        yes;
                                prime2Mean  yes;
                                base        time;
                        }
			T.gas
                        {
                                mean        yes;
                                prime2Mean  yes;
                                base        time;
                        }
			T.particles
                        {
                                mean        yes;
                                prime2Mean  yes;
                                base        time;
                        }			
		);
    		writeControl    writeTime;
		timeStart	0.05;
		timeEnd		1;
	}	
}
