FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitTparticles
{
    type       limitTemperature;
    min        200;
    max        8000;
    selectionMode all;
    phase		particles;
}

limitTgas
{
    type       limitTemperature;
    min        200;
    max        8000;
    selectionMode all;
    phase		gas;
}


