(
  (particles in gas)
  {
    type            entrainedInterfaceMotion;
    propellant      propellant;
    n               0.405;
    a               0.992;
    f               1.0;
    Pmin	    10;
    isoAlpha 	    0.5;
    MR		    0;
  }
);

