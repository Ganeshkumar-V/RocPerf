{
    type            pureParticlePhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d               80e-9;
    }
    residualAlpha   1e-15;
}

propellant
{
    type            pureIsothermalStaticPhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d           80e-9;
    }
    residualAlpha   1e-15;
}

