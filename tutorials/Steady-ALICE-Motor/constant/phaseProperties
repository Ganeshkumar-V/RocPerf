FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            gasParticleMultiphaseSystem;

phases          (gas particles);

particlePhase	particles;

gasPhase	gas;

propellantIndex -1; // no propellant phase

limitTemperature     true;  // To limit temperature wihtin limits (minTemp, maxTemp) change the following to true
minTemp		           300;    // Minimum temperature limit
maxTemp		           2500;   // Maximum temperature limit

gas
{
    type            pureThermalEnergyPhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d               80e-9;
    }
    residualAlpha   1e-15;
}

particles
{
    type            pureParticlePhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d               80e-9;
    }
    residualAlpha   1e-15;
}

propellant
{
    type            pureIsothermalStaticPhaseModel;
    diameterModel   constant;
    constantCoeffs
    {
        d           80e-9;
    }
    residualAlpha   1e-15;
}

interfaceTracking
();

sharpInterfaceHeatTransfer
(
  (particles in gas)
  {
    type            KavanauRanzMarshall;
    residualAlpha   1e-6;
    cutoff          1e-15;
    gammaR          3697.6;
  }
);

drag
(
  (particles in gas)
  {
    type            Loth;
    residualRe      1e-15;
    R	 	    2854.4;
    gamma	    1.2954;
    swarmCorrection
    {
    	type        none;
    }
  }
);

blending
{
    default
    {
      type            none;
      continuousPhase gas;
    }
}

surfaceTension
();

aspectRatio
();

virtualMass
();

interfaceComposition
();

interfaceCompression
();

// Models used to find Mass Transfer Coefficient for component transfer within the gas phase
massTransfer.gas
();

massTransfer.propellant
();

heatTransfer.particles
();

heatTransfer.gas
();

heatTransfer.propellant
();

phaseTransfer
();

lift
();

wallLubrication
();

turbulentDispersion
();

// Minimum allowable pressure
pMin            10;


// ************************************************************************* //
